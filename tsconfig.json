{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es5",
		"noImplicitAny": true,
		"removeComments": true,
		"moduleResolution": "node",
		"sourceMap": false,
		"outDir": "run",
		"baseUrl": ".",
		"paths": {
			"*": [
				"node_modules/*",
				"src/types/*"
			]
		}
	},
	"include": [
		"src/**/*"
	],
	"exclude": [
		"node_modules"
	]
}
